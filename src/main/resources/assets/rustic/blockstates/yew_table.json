{
  "multipart": [
    {
      "apply": {
        "model": "rustic:block/yew_table"
      }
    },
    {
      "when": {
        "nw": "true"
      },
      "apply": {
        "model": "rustic:block/yew_table_leg"
      }
    },
    {
      "when": {
        "ne": "true"
      },
      "apply": {
        "model": "rustic:block/yew_table_leg",
        "y": 90
      }
    },
    {
      "when": {
        "se": "true"
      },
      "apply": {
        "model": "rustic:block/yew_table_leg",
        "y": 180
      }
    },
    {
      "when": {
        "sw": "true"
      },
      "apply": {
        "model": "rustic:block/yew_table_leg",
        "y": 270
      }
    }
  ]
}